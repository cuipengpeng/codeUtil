<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/circle_corner_gray_border_normal_2dp"
        android:layout_margin="10dp">
        <LinearLayout
            android:id="@+id/ll_calibrationStartHorizontalActivity_legend"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="25dp">
            <RelativeLayout
                android:id="@+id/rl_calibrationStartHorizontalActivity_legend01"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/iv_calibrationStartHorizontalActivity_circleIcon01"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:layout_centerInParent="true"
                    android:background="@drawable/circle_corner_blue_bg_normal_2dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_toRightOf="@id/iv_calibrationStartHorizontalActivity_circleIcon01"
                    android:layout_marginLeft="5dp"
                    android:text="角度"
                    android:textSize="@dimen/appTipTextSize_13sp"
                    android:gravity="center_vertical"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_calibrationStartHorizontalActivity_legend02"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/iv_calibrationStartHorizontalActivity_circleIcon02"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:layout_centerInParent="true"
                    android:background="@drawable/circle_corner_yellow_bg_normal_2dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_toRightOf="@id/iv_calibrationStartHorizontalActivity_circleIcon02"
                    android:layout_marginLeft="5dp"
                    android:text="速度"
                    android:textSize="@dimen/appTipTextSize_13sp"
                    android:gravity="center_vertical"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_calibrationStartHorizontalActivity_legend03"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/iv_calibrationStartHorizontalActivity_circleIcon03"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:layout_centerInParent="true"
                    android:background="@drawable/circle_corner_green_bg_normal_2dp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_toRightOf="@id/iv_calibrationStartHorizontalActivity_circleIcon03"
                    android:layout_marginLeft="5dp"
                    android:text="交互力"
                    android:textSize="@dimen/appTipTextSize_13sp"
                    android:gravity="center_vertical"/>
            </RelativeLayout>
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="2dp"
            android:layout_below="@id/ll_calibrationStartHorizontalActivity_legend">
            <com.github.mikephil.charting.charts.LineChart
                android:id="@+id/lc_calibrationStartHorizontalActivity_lineChart"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="20dp"
                android:background="#ffffff"/>
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>